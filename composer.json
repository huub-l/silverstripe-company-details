{
  "name": "silverstripe-company-details",
  "description": "Provides extra settings",
  "type": "silverstripe-vendormodule",
  "homepage": "https://github.com/huubl/silverstripe-company-details",
  "keywords": ["silverstripe", "Contact", "Details", "Social", "Media"],
  "authors": [
    {
      "name": "Huubl",
      "email": "contact@studiolunter.nl"
    }
  ],
  "require": {
    "silverstripe/framework": "~4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7"
  },
  "extra": {
    "expose": [
      "javascript"
    ]
  },
  "autoload": {
    "psr-4": {
      "huubl\\silverstripe-company-details\\": "src/"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev"
}
